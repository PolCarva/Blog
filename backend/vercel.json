{
  "name": "multimedia-social-media",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/uploads/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "dest": "uploads/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
