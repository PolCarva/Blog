{
    "name": "multimedia-social-media",
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
  }